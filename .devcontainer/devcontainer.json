{
  "service": "web",
  "dockerComposeFile": "docker-compose.yml",
  "mounts": [
    {
      "source": "..",
      "target": "/var/www/studip/public/plugins_packages/fstutzky/LogicExercises",
      "type": "volume"
    }
  ],
  "workspaceFolder": "/var/www/studip/public/plugins_packages/fstutzky/LogicExercises",
  "shutdownAction": "stopCompose",
  "postCreateCommand": ["bash", "-i", "-c", "fnm install && npm install"],
  "postAttachCommand": ["bash", "-i", "-c", "fnm use"],
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "github.vscode-pull-request-github",
        "github.copilot"
      ]
    }
  }
}
